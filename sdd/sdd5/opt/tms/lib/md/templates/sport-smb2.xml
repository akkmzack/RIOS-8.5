
   <!-- smbv2 settings -->

    <smb2
        enabled="@/rbt/sport/smb2/config/enable@"
        force_basic_dialect="@/rbt/sport/smb2/config/force_basic_dialect@"
        ss_after_idle="@/rbt/sport/smb2/config/ss_after_idle@"
        allow_neg_create_resp_from_ff_data="@/rbt/sport/smb2/config/negative_response_from_find_data@"
        applock_enabled="@/rbt/sport/smb2/config/applock@"
        strip_8dot3="@/rbt/sport/smb2/config/strip_8dot3@"
        >

        <smb3_support
            enabled="@/rbt/sport/smb2/config/smb3/enable@"/>

        <negotiate_whitelist
            enabled="@/rbt/sport/smb2/config/negotiate_whitelist/enable@"
            dialects="@/rbt/sport/smb2/config/negotiate_whitelist/dialects@"/>

        <nodes
            per_garbage_collection="@/rbt/sport/smb2/config/nodes/per_garbage_collection@"/>

        <mdata_timeout
            minval_msec="@/rbt/sport/smb2/config/metadata/timeout/minimum@"
            maxval_msec="@/rbt/sport/smb2/config/metadata/timeout/maximum@"
            multiplier="@/rbt/sport/smb2/config/metadata/timeout/multiplier@"/>

        <short_name_checking
            enabled="@/rbt/sport/smb2/config/short_name_checking/enable@"/>

        <access_check
            enabled="@/rbt/sport/smb2/config/access_check/enable@"
            clear_timestamps="@/rbt/sport/smb2/config/access_check/clear_timestamp@"/>

        <mdata_cache
            fsinfo_caching="@/rbt/sport/smb2/config/metadata/cache/fsinfo_caching@"
            secinfo_prefetch="@/rbt/sport/smb2/config/metadata/cache/secinfo_prefetch@"/>

        <disk_threshold_check
            enabled="@/rbt/sport/smb2/config/disk_threshold/enable@"
            poll_period_sec="@/rbt/sport/smb2/config/disk_threshold/poll_period@"
            low_water_mb="@/rbt/sport/smb2/config/disk_threshold/low_watermark@"
            high_water_mb="@/rbt/sport/smb2/config/disk_threshold/high_watermark@"/>

        <write_behind
            enabled="@/rbt/sport/smb2/config/write_behind/enable@"
            global_limit_kb="@/rbt/sport/smb2/config/write_behind/global_limit@"
            connection_limit_kb="@/rbt/sport/smb2/config/write_behind/connection_limit@"
            request_limit="@/rbt/sport/smb2/config/write_behind/request_limit@"/>

        <read_ahead
            enabled="@/rbt/sport/smb2/config/read_ahead/enable@"
            prefetch_size_mb="@/rbt/sport/smb2/config/read_ahead/prefetch_size@"
            holdback_window_mb="@/rbt/sport/smb2/config/read_ahead/holdback_window@"
            max_random_seq_window_kb="@/rbt/sport/smb2/config/read_ahead/max_random_seq_window@"
            increment_size_mb="@/rbt/sport/smb2/config/read_ahead/increment_size@"
            read_req_size_kb="@/rbt/sport/smb2/config/read_ahead/read_request_size@"/>

        <batch_predictor
            enabled="@/rbt/sport/smb2/config/batch_prediction/enable@"
            hit_count="@/rbt/sport/smb2/config/batch_prediction/hit_count@"
            batch_size="@/rbt/sport/smb2/config/batch_prediction/batch_size@"
            read_ahead_size_kb="@/rbt/sport/smb2/config/batch_prediction/read_ahead_size@"
            object_timeout_sec="@/rbt/sport/smb2/config/batch_prediction/object_timeout@"/>

        <blacklist
            timeout_minutes="@/rbt/sport/smb2/config/blacklist/timeout@"/>

        <credit_mgr
            max_synthetic_credits="@/rbt/sport/smb2/config/credit_manager/max_synthetic@"
            good_client_credits="@/rbt/sport/smb2/config/credit_manager/good_client@"
            throttle_credits="@/rbt/sport/smb2/config/credit_manager/throttle_credits@"
            max_client_credits="@/rbt/sport/smb2/config/credit_manager/max_client_credits@"/>

        <data_mgr
            enabled="@/rbt/sport/smb2/config/data_manager/enable@"
            mem_limit_mb="@/rbt/sport/smb2/config/data_manager/mem_limit@"
            encode_limit_kb="@/rbt/sport/smb2/config/data_manager/encode_limit@"
            encode_max_txn="@/rbt/sport/smb2/config/data_manager/encode_max_txn@"
            retain_data_without_handles="@/rbt/sport/smb2/config/data_manager/retain_data@"/>

        <find_responses
            negative_single="@/rbt/sport/smb2/config/find_responses/negative_single@"/>

        <find_caching
            enabled="@/rbt/sport/smb2/config/find_caching/enable@"
            mem_limit_mb="@/rbt/sport/smb2/config/find_caching/mem_limit@"
            max_bytes_per_dir="@/rbt/sport/smb2/config/find_caching/max_bytes_per_dir@"/>

        <find_prefetch
            enabled="@/rbt/sport/smb2/config/find_prefetch/enable@"
            window_size_kb="@/rbt/sport/smb2/config/find_prefetch/window_size@"/>

        <local_ioctl_resp
            unsupported="@/rbt/sport/smb2/config/local_ioctl_resp/unsupported@"
            invalid="@/rbt/sport/smb2/config/local_ioctl_resp/invalid@"/>

        <ipc_opt
            getinfo="@/rbt/sport/smb2/config/ipc_opt/getinfo@"
            ioctl="@/rbt/sport/smb2/config/ipc_opt/ioctl@"/>

        <idle_foi
            enabled="@/rbt/sport/smb2/config/idle_foi/enable@"
            timeout_msec="@/rbt/sport/smb2/config/idle_foi/timeout@"/>

        <fix_server
            notify_header="@/rbt/sport/smb2/config/fix_server/notify_header@"
            set_root_dir_bit="@/rbt/sport/smb2/config/fix_server/set_root_dir_bit@"/>

    </smb2>
