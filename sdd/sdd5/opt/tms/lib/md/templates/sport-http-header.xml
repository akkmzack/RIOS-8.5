       profile_tbl_size="@/rbt/sport/blade/http/config/auto_config/size@"
       trans_threshold="@/rbt/sport/blade/http/config/auto_config/transaction_threshold@"
       http_kerberos_support="@/rbt/sport/blade/http/config/native_krb/enable@"
       prefetch_on_set_cookie="@/rbt/sport/blade/http/config/prefetch_on_set_cookie@"
