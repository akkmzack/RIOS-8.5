       max_auth_cache_time="@/rbt/sport/blade/http/config/auth_cache/max_time@"
       max_auth_cache_count="@/rbt/sport/blade/http/config/auth_cache/max_count@">

        <webdav_appblade>
            <local_response
                enable="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/local_response/enable@"/>
            <depth_upgrade
                enable="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/depth_upgrade/enable@"/>

            <webdav_metadata_cache
                rtt_multiplier="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/multiplier@"
                max_count="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/max_count@"
                max_entry_size_bytes="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/max_entry_size@"
                max_cache_size_bytes="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/max_cache_size@"
                max_time_msecs="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/max_time@"
                min_time_msecs="@/rbt/sport/blade/http/appblade/sharepoint/webdav/config/metadata_cache/min_time@"
            />
        </webdav_appblade>

        <fpse_appblade>
            <server_version
                enable="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/server_version/enable@"/>
            <open_service
                enable="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/open_service/enable@"/>
            <url_to_web_url
                enable="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/url_to_web_url/enable@"/>
            <options
                enable="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/options/enable@"/>
            <vti_info
                enable="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/vti_info/enable@"/>

            <fpse_metadata_cache
                max_count="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/metadata_cache/max_count@"
                max_entry_size_bytes="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/metadata_cache/max_entry_size@"
                max_cache_size_bytes="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/metadata_cache/max_cache_size@"
                cache_time_secs="@/rbt/sport/blade/http/appblade/sharepoint/fpse/config/metadata_cache/cache_time@"
            />
        </fpse_appblade>

       <supported_port port="80" enable="true" />
       <supported_port port="8080" enable="true" />
       <supported_port port="443" enable="true" />
