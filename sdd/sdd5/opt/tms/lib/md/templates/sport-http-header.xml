       inflight_mode="@inflight_mode@"
       max_inflight_cache_size="@/rbt/sport/blade/http/config/stream_splitting/max_cache_size@"
       inflight_cache_delay_msecs="@/rbt/sport/blade/http/config/stream_splitting/inflight_cache_delay_msecs@"
       ignore_inflight_cache_control="@/rbt/sport/blade/http/config/stream_splitting/ignore_inflight_cache_control@"
       ignore_query_string_params="@/rbt/sport/blade/http/config/stream_splitting/ignore_query_string@"
