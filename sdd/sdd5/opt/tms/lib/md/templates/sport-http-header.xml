       heuristic_lifetime="@/rbt/sport/blade/http/config/cache/heuristic_lifetime@"
       no_200_cache="@/rbt/sport/blade/http/config/cache/no_200_cache@"
       allow_space_in_uri="@/rbt/sport/blade/http/config/allow_space_in_uri@"
       cache_without_auth="@/rbt/sport/blade/http/config/cache_without_auth@"
       bypass_post_req_channel="@/rbt/sport/blade/http/config/bypass_post_req_channel@"
       low_mem_bypass="@/rbt/sport/blade/http/config/low_mem_bypass/enable@"
