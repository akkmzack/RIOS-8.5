
   <!-- NFS -->

   <nfs
       enabled="@/rbt/sport/nfs/config/enabled@"
       v2_v4_alarm_enabled="@/rbt/sport/nfs/config/v2_v4_alarm_enabled@"
       call_timeout="@/rbt/sport/nfs/config/call_timeout@"
       nfs_cache_timer_freq="@/rbt/sport/nfs/config/nfs_cache_timer_freq@"
       nfs_retry_timer="@/rbt/sport/nfs/config/nfs_retry_timer_freq@"
       suspend_margin="@/rbt/sport/nfs/config/suspend_margin@"
       memory_soft_limit="@/rbt/sport/nfs/config/memory_soft_limit@"
       memory_hard_limit="@/rbt/sport/nfs/config/memory_hard_limit@"
       max_directory_count="@/rbt/sport/nfs/config/max_directory_count@"
       max_directory_prefetch_size="@/rbt/sport/nfs/config/max_directory_prefetch_size@"
       max_symlink_count="@/rbt/sport/nfs/config/max_symlink_count@"
       nfs_block_immediate="@/rbt/sport/nfs/config/nfs_block_immediate/enable@"
       encode_limit_enable="@/rbt/sport/nfs/config/encode_limit_enable/enable@"
       encode_limit="@/rbt/sport/nfs/config/encode_limit@"
       dir_blacklist_enable="@/rbt/sport/nfs/config/dir_blacklist_enable@"
       dir_blacklist_timeout="@/rbt/sport/nfs/config/dir_blacklist_timeout@"
       dir_blacklist_rate_limit="@/rbt/sport/nfs/config/dir_blacklist_rate_limit@">

      <nfs_server
          default="true"
          name="default"
          policy="@/rbt/sport/nfs/config/server/default/policy@"
          write_optimize="@/rbt/sport/nfs/config/server/default/write_optimize@"
          readdir_optimize="@/rbt/sport/nfs/config/server/default/readdir_optimize@"
          drc_capacity="@/rbt/sport/nfs/config/server/default/drc_capacity@"
          xid_map_all="@/rbt/sport/nfs/config/server/default/xid_map_all@"
          rah_threshold="@/rbt/sport/nfs/config/server/default/rah_threshold@"
          rah_transfer_size="@/rbt/sport/nfs/config/server/default/rah_transfer_size@"
          threshold_multiple="@/rbt/sport/nfs/config/server/default/threshold_multiple@"
          small_file_readahead="@/rbt/sport/nfs/config/server/default/small_file_readahead@"
          rah_no_suspend="@/rbt/sport/nfs/config/server/default/rah_no_suspend@"
          max_history_files="@/rbt/sport/nfs/config/server/default/max_history_files@"
          client_history_len="@/rbt/sport/nfs/config/server/default/client_history_len@"
          readdir_read_size="@/rbt/sport/nfs/config/server/default/readdir_read_size@"
          enable_direntry_map="@/rbt/sport/nfs/config/server/default/enable_direntry_map@"
          max_write_data="@/rbt/sport/nfs/config/server/default/max_write_data@"
          max_slow_write_calls="@/rbt/sport/nfs/config/server/default/max_slow_write_calls@">

         <nfs_volume
             default="true"
             policy="@/rbt/sport/nfs/config/server/default/volume/default/policy@"
             close_to_open="@/rbt/sport/nfs/config/server/default/volume/default/close_to_open@"
             dir_idle_time="@/rbt/sport/nfs/config/server/default/volume/default/dir_idle_time@"
             dir_getattr_threshold="@/rbt/sport/nfs/config/server/default/volume/default/dir_getattr_threshold@"
             data_lifetime="@/rbt/sport/nfs/config/server/default/volume/default/data_lifetime@"
             metadata_lifetime="@/rbt/sport/nfs/config/server/default/volume/default/metadata_lifetime@"
             root_squash_enabled="@/rbt/sport/nfs/config/server/default/volume/default/root_squash_enabled@" 
             nfs_perm_cache_enabled="@/rbt/sport/nfs/config/server/default/volume/default/permission_cache@" />

      </nfs_server>

