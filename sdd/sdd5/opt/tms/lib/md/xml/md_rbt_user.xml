    </config>
    <config>
        <name>/user/*/sh/web/reports/snapmirror/traffic_type</name>
        <description>SnapMirror Traffic Type (LAN/WAN)</description>
        <value type="string">LAN</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
