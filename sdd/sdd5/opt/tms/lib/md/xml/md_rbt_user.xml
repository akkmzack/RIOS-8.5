        <description>UI category</description>
        <value type="string">0</value>
        <db-policy>localdb</db-policy>
    </config>
    <!-- end Current Connections -->

    <config>
        <name>/user/*/sh/web/reports/nfs_server_ip</name>
        <description>IP Address of NFS Server</description>
        <value type="ipv4addr">0.0.0.0</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/srdf/symm_and_rdf_group</name>
        <description>Symmetrix (RDF Group)</description>
        <value type="string">All:255</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
