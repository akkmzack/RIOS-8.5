    <config>
        <name>/user/*/sh/web/reports/appvis/interface</name>
        <description>Application Visibility interface setting</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/appvis/graphedapps</name>
        <description>Application Visibility graphed applications setting</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
