        <description>Established after this timestamp</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/current_connections/started_before</name>
        <description>Established before this timestamp</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/current_connections/transport_type</name>
        <description>Transport type</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
