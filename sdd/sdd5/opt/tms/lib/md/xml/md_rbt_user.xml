<node-list module="user">
    <config>
        <name>/user/*/sh/web/http/static_filter</name>
        <description>HTTP Static Filter</description>
        <value type="bool">true</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/http/auto_filter</name>
        <description>HTTP Auto Filter</description>
        <value type="bool">true</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/http/eval_filter</name>
        <description>HTTP Eval Filter</description>
        <value type="bool">false</value>
        <db-policy>localdb</db-policy>
    </config>

    <!-- Nodes for controls for Reports 2.0 graphs. -->

    <!-- QoS -->
    <config>
        <name>/user/*/sh/web/reports/qos/inbound/classes</name>
        <description>QoS Inbound Classes</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/qos/outbound/classes</name>
        <description>QoS Outbound Classes</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/qos/inbound/graph_type</name>
        <description>QoS Inbound Graph Type</description>
        <value type="string">total</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/qos/outbound/graph_type</name>
        <description>QoS Outbound Graph Type</description>
        <value type="string">total</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/qos/units</name>
        <description>QoS Units</description>
        <value type="string">packets</value>
        <db-policy>localdb</db-policy>
    </config>

    <!-- VNI -->
    <config>
        <name>/user/*/sh/web/reports/vniName</name>
        <description>VNI Name</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>

    <!-- RSP -->
    <config>
      <name>/user/*/sh/web/reports/rspUnit</name>
        <description>RSP Units (packets/sec | bps)</description>
        <value type="string">bps</value>
        <db-policy>localdb</db-policy>
    </config>

    <!-- Traffic -->
    <config>
        <name>/user/*/sh/web/reports/traffic_port</name>
        <description>Traffic Port</description>
        <value type="uint16">0</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/traffic_direction</name>
        <description>Traffic Direction</description>
        <value type="string">bi</value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/neighbor_ip</name>
        <description>IP Address of Neighbor</description>
        <value type="ipv4addr">0.0.0.0</value>
        <db-policy>localdb</db-policy>
    </config>

