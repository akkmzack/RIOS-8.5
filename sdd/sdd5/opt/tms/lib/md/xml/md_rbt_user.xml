    </config>
    <config>
        <name>/user/*/sh/web/reports/current_connections/src_port</name>
        <description>Source port</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
    </config>
    <config>
        <name>/user/*/sh/web/reports/current_connections/src_subnet</name>
        <description>Source IPv6 prefix</description>
        <value type="string"></value>
        <db-policy>localdb</db-policy>
