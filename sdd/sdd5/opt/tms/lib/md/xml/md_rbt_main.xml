<node-list module="rbt_main">
   <config>
      <name>/rbt/sport/main/config/cpu_scale</name>
      <description>CPU Scale factor value</description>
      <value type="float32" max="65535.0" min="0.0">1.0</value>
   </config>
   <config>
      <name>/rbt/sport/main/config/nagle_blade/enable</name>
      <description>Nagle blade enabled status</description>
      <value type="bool">true</value>
   </config>
   <config>
      <name>/rbt/sport/main/config/nagle_blade/timeout</name>
      <description>Nagle blade timeout</description>
      <value type="uint32">1</value>
   </config>
   <config>
      <name>/rbt/sport/main/config/nagle_blade/nagle_len</name>
      <description>Nagle blade nagle length</description>
      <value type="uint32">131072</value>
   </config>
</node-list>
