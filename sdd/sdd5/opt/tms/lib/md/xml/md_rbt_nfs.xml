<node-list module="rbt_sport_nfs">
    <config>
        <name>/rbt/sport/nfs/config/max_directory_prefetch_size</name>
        <description>Max directory prefetch amount</description>
        <value type="uint32">2048</value>
    </config>
    <config>
        <name>/rbt/sport/nfs/config/nfs_retry_timer_freq</name>
        <description>Retry timer frequency</description>
        <value type="uint32">4000</value>
    </config>
    <config>
        <name>/rbt/sport/nfs/config/dir_blacklist_enable</name>
        <description>Enable directory blacklist</description>
        <value type="bool">true</value>
    </config>
    <config>
        <name>/rbt/sport/nfs/config/dir_blacklist_timeout</name>
        <description>Directory blacklist timeout</description>
        <value type="uint16">600</value>
    </config>
    <config>
        <name>/rbt/sport/nfs/config/dir_blacklist_rate_limit</name>
        <description>Directory blacklist rate limit</description>
        <value type="uint32">60</value>
    </config>
</node-list>
