     client_ntlmv2="yes"
     ldap_ssl="no"
     machine_pw_timeout="0"
     interfaces="primary lo aux"
     bind_interfaces_only="yes"
     disable_ipv6_lookups="yes"
     use_tdb_mmap="yes"
     netbios_name=""
/>
  <script     run_freq_sec="55"
     max_processes="3"
     max_process_life_span_sec="600"
/>
  <helperd     tdb_backup_freq_sec="600"
/>
