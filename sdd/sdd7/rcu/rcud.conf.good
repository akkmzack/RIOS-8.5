     use_netbios="true"
     alarm_domain_join_failure="0"
     max_mpx_pct="40"
     ignore_pct="0"
     dialect_check="true"
     memory_limit="512"
/>
  <domain>
    <dom_config       login="administrator"
       join_type="workstation"
/>
