/>
  </domain>
  <smb_global     server_signing="disabled"
     max_smbd_processes="142"
     workgroup="WGC"
     realm="WGC.COM"
     nt_acl_support="yes"
     server_string="Proxy File Services"
     log_file="/var/log/samba/smbd.log"
