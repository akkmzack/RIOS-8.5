     admin_users="@root"
     local_master="no"
     domain_master="no"
     preferred_master="no"
     os_level="0"
     idmap_uid="16777216-33554431"
     idmap_gid="16777216-33554431"
     template_shell="/bin/false"
     winbind_use_default_domain="no"
     winbind_enum_users="yes"
     winbind_enum_groups="yes"
     winbind_ignore_trusted_domains="no"
