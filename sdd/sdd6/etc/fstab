LABEL=ROOT_2	/	ext3	defaults,noatime	1 1
LABEL=BOOT_2	/boot	ext3	defaults,noatime	1 2
LABEL=BOOTMGR	/bootmgr	ext3	defaults,noatime	1 2
LABEL=CONFIG	/config	ext3	defaults,noatime	1 2
/dev/disk0p2	swap	swap	defaults	0 0
LABEL=VAR	/var	ext3	defaults	1 2
none		/proc           proc    defaults        0 0
none		/dev/pts        devpts  gid=5,mode=620  0 0
none		/dev/shm        tmpfs   defaults        0 0
none		/tmp            tmpfs   size=16M        0 0
/dev/cdrom	/mnt/cdrom      iso9660 noauto,ro       0 0
/dev/fd0	/mnt/floppy     auto    noauto          0 0
