
   <!-- Notes -->

   <notes
       enable="@/rbt/sport/notes/config/enable@"
       port="@/rbt/sport/notes/config/port@"
       enable_transport_compression_opt="@/rbt/sport/notes/config/compress@"
       enable_attach_upload_opt="@/rbt/sport/notes/config/attach@"
       enable_attach_decompression_opt="@/rbt/sport/notes/config/attach_comp@"
       queue_limit="@/rbt/sport/notes/config/queue@"
       enable_pullrepl_decompression_opt="@/rbt/sport/notes/config/pullrepl@"
       preack_limit="@/rbt/sport/notes/config/preack_limit@"/>

   <notesencr
       enable="@/rbt/sport/notes/config/encrypt/enable@"
       allow_anonymous="@/rbt/sport/notes/config/encrypt/anonymous/enable@"
       ca_certs_dir="/var/opt/rbt/decrypted/notes/certs"
       bypass_on_error="@/rbt/sport/notes/config/encrypt/bypass_on_error/enable@"/>

   <notesencr2
       enable="@/rbt/sport/notes/config/encrypt_v2/enable@"
       server_port="@/rbt/sport/notes/config/encrypt_v2/server_port@"
       blacklist_timeout="@/rbt/sport/notes/config/encrypt_v2/blacklist_timeout@"
       pending_conn_timeout="@/rbt/sport/notes/config/encrypt_v2/pending_conn_timeout@"
       recent_conn_timeout="@/rbt/sport/notes/config/encrypt_v2/recent_conn_timeout@"
       ca_server_ids_dir="/var/opt/rbt/decrypted/notes/server_certs"/>
