<node-list module="rbt_tds">
  <config>
    <name>/rbt/sport/tds/config/auto_bypass</name>
    <description>Auto-bypass future connections on failure</description>
    <value type="bool">true</value>
  </config>

  <config>
    <name>/rbt/sport/tds/config/bypass_timeout</name>
    <description>Amount of time to bypass on a failed connection</description>
    <value type="uint32">3600</value>
  </config>
</node-list>
