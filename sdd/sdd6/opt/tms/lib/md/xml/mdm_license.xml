<node-list module="license">

<config>
        <name>/license/client/config/renew_interval</name>
        <description>Current renew interval in seconds</description>
        <value type="duration_sec">60</value>
        <db-policy>localdb</db-policy>
</config>

<config>
        <name>/license/client/config/last_state_change_reason</name>
        <description>Reason for last state change</description>
        <value type="string">Appliance is uninitialized.</value>
        <db-policy>localdb</db-policy>
</config>

<config>
        <name>/license/client/config/last_state_change_time</name>
        <description>Time of last state change</description>
        <value type="uint32">0</value>
        <db-policy>localdb</db-policy>
</config>

</node-list>
